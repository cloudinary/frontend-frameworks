{
  "name": "@cloudinary/svelte",
  "version": "1.0.0-beta.3",
  "description": "A Svelte component library for Cloudinary",
  "author": "Cloudinary",
  "homepage": "https://cloudinary.com",
  "license": "MIT",
  "repository": "https://github.com/cloudinary/frontend-frameworks",
  "bugs": {
    "url": "https://github.com/cloudinary/frontend-frameworks/issues"
  },
  "svelte": "dist/index.umd.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "umd": "./dist/index.umd.js",
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "svelte-check": "svelte-check",
    "build": "npm run build --prefix ../html && rollup -c && cp package.json ./dist",
    "test": "npm run test:unit && npm run test:ssr",
    "test:unit": "jest",
    "test:ssr": "jest -c jest-ssr.config.json",
    "test-coverage": "jest --coverage"
  },
  "keywords": [
    "svelte",
    "cloudinary"
  ],
  "files": [
    "src",
    "dist"
  ],
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@cloudinary/base": "^1.0.0-beta.2",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "rollup": "^2.21.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "svelte": "^3.32.1",
    "svelte-check": "^1.1.32",
    "svelte-jester": "^1.3.0",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.6.6",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^3.9.7",
    "wait-for-expect": "^3.0.2",
    "@rollup/plugin-json": "^4.1.0"
  },
  "dependencies": {
    "@cloudinary/html": "1.0.0-beta.2"
  }
}
